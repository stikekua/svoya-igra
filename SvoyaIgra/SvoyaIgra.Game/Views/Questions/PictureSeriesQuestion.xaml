<UserControl x:Class="SvoyaIgra.Game.Views.Questions.PictureSeriesQuestion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SvoyaIgra.Game.Views.Questions"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel VerticalAlignment="Center">
        <Label Content="I am picture series" FontSize="20"/>
        <TextBlock Text="{Binding CurrentQuestion.QuestionText}" FontSize="30"/>
    </StackPanel>
</UserControl>
